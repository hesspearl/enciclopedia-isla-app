{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx", 
    "lib": ["DOM", "ESNext"], 
    "moduleResolution": "Node",
     "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src","vite-env.d.ts", "scripts"],
  "exclude": ["node_modules"],
  "parserOptions": {
  "project": "./tsconfig.json",
 
  "sourceType": "module"
}

}